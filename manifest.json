{
    "name": "g00bygui",
    "version": "1.0",
    "manifest_version": 3,
    "description": "An experimental Websim extension",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://*.websim.ai/*"],
            "js": ["content.js"]
        }
    },
    "icons": {
        "128": "g00bygui_icon.png"
    }
}
