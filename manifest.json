{
    "name": "g00bygui",
    "version": "1.0",
    "manifest_version": 2,
    "description": "An experimental Websim extension",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["*://*.websim.ai/*"],
            "js": ["content.js"]
        }
    },
    "icons": {
        "128": "g00bygui_icon.jpeg"
    }
}
